issn = {1935-2735},
journal = {PLoS Neglected Tropical Diseases},
keywords = {Budgets,Dengue fever,Disease vectors,Infectious disease control,Infectious disease surveillance,Public and occupational health,Vector-borne diseases,West Nile virus},
month = {oct},
number = {10},
pages = {e858},
publisher = {Public Library of Science},
title = {{Unforeseen Costs of Cutting Mosquito Surveillance Budgets}},
