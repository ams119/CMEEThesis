keywords = {Ochlerotatus j. japonicus,West Nile virus,dairy farms,invasive species,mosquito vector,public health},
month = {jun},
number = {2},
pages = {171--176},
publisher = {The American Mosquito Control Association},
title = {{The importance of agricultural tire habitats for mosquitoes of public health importance in New York State}},
url = {https://bioone.org/journals/Journal-of-the-American-Mosquito-Control-Association/volume-21/issue-2/8756-971X(2005)21[171:TIOATH]2.0.CO;2/THE-IMPORTANCE-OF-AGRICULTURAL-TIRE-HABITATS-FOR-MOSQUITOES-OF-PUBLIC/10.2987/8756-971X(2005)21[171:TIOATH]2.0.CO;2.fu},
