issn = {1742-5689},
journal = {Journal of The Royal Society Interface},
keywords = {Adaptive and non-adaptive sampling design,Model-based geostatistics,Mosquito sampling,Remote sensing and field data,Stratification,Sub-Saharan Africa},
month = {apr},
